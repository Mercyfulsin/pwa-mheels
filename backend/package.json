    "google-maps-react": "^2.0.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.10",
    "morgan": "^1.9.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-input-switch": "^2.2.1",
